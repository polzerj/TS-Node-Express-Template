{
    "compilerOptions": {
        "sourceMap": true,
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "baseUrl": "./src",
        "paths": {
            "@tsEx/types": ["types/types"],
            "@tsEx/services/*": ["services/*"],
            "@tsEx/data/*": ["data/*"],
            "@tsEx/models/*": ["models/*"]
        }
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}
